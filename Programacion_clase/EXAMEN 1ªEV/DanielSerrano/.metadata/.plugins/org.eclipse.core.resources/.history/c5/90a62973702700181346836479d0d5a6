package Ej2;

public class Prueba {

	public static void main(String[] args) {
		double num=23.12;
		char a=' ';
		String completo=String.valueOf(num);
		String entero="";
		do {
			for (int i = 0; i < completo.length(); i++) {
				
				a = completo.charAt(i);
				
				entero = entero + String.valueOf(a);
			}
			
		} while (a!='.');
		System.out.println(entero);
	}

}
