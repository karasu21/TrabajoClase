package Ej5;

public class Prueba {

	public static void main(String[] args) {
		System.out.println("t");
		String palabra = Entrada.cadena();
		String cifrado="";
		char a;
		System.out.println(palabra);
		for (int i = 0; i < palabra.length(); i++) {
			a = palabra.charAt(i);
			a = (char) (a + 3);
			cifrado=cifrado+String.valueOf(a);
		}
System.out.println(cifrado);
	}

}
