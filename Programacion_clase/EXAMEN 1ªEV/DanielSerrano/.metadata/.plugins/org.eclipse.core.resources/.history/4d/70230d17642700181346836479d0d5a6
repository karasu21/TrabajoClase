package Ej1;

public class Inicio {

	public static void main(String[] args) {
		Trabajador n = new Trabajador("Juan", 11, 0, 0);
		System.out.println(n);
		System.out.println(n.calcularSueldo());
		
		Trabajador n = new Trabajador("Pepe", 11, 1, 1);
		System.out.println(n);
		System.out.println(n.calcularSueldo());
		
	}

}
