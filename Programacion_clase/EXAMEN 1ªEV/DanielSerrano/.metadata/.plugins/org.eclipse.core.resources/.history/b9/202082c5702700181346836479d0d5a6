package Ej2;

public class Prueba {

	public static void main(String[] args) {
		double num=23.12;
		int cnt=0;
		char a=' ';
		String completo=String.valueOf(num);
		String entero="";
		do {
				
				cnt=cnt+1;
				entero = entero + String.valueOf(a);
				a = completo.charAt(cnt);
				
				
			
			
		} while (a!='.');
		System.out.println(entero);
	}

}
