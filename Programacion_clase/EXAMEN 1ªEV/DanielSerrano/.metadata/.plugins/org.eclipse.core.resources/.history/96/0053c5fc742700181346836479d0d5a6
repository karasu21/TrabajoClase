package Ej3;

public class CoeficientesBinomiales {

	public static void main(String[] args) {
		int n;
		int k;
		do {
			System.out.println("introduzca valor de n (n,k)");
			n=Entrada.entero();
			System.out.println("introduzca valor de k (n,k)");
			k=Entrada.entero();
		} while (n<0 || k<0);
		
	}

}
